(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.+ (re.union (re.* (re.+ (str.to.re "a"))) (str.to.re "b")))))
(assert (str.in.re y (re.+ (re.++ (str.to.re "x") (str.to.re "{")))))
(assert (= (str.len x) 3))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
