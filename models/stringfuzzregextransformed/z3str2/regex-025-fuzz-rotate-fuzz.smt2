(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.+ (re.union (re.* (str.to.re "")) (str.to.re "w")))))
(assert (str.in.re y (re.* (re.union (re.+ (str.to.re "b")) (str.to.re "a")))))
(assert (= (str.len x) (str.to.int y)))
(assert (not (= x y)))
(assert (= (str.to.int x) 3))
(check-sat)
