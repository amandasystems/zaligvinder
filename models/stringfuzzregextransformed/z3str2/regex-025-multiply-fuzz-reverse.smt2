(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.+ (re.++ (str.to.re "bTU") (re.* (str.to.re "P"))))))
(assert (str.in.re y (re.+ (re.union (re.+ (str.to.re "aa")) (str.to.re "")))))
(assert (= (str.len x) (str.to.int y)))
(assert (not (= x y)))
(assert (= (str.len x) 6))
(check-sat)
