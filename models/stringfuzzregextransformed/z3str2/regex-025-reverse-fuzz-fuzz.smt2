(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.++ (str.to.re "") (re.+ (str.to.re "L"))))))
(assert (str.in.re y (re.* (re.union (str.to.re "") (re.+ (str.to.re ""))))))
(assert (= (str.len x) (str.to.int y)))
(assert (not (= x y)))
(assert (= (str.to.int x) 0))
(check-sat)
