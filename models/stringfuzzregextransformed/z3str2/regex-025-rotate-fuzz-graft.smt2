(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.++ (re.+ (str.to.re "b")) (str.to.re "")))))
(assert (str.in.re y (re.* (re.union (str.to.re "a") (re.* (str.to.re "X"))))))
(assert (= 11 (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) (str.len x)))
(check-sat)
