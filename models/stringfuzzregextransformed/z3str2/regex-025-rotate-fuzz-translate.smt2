(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.++ (re.+ (str.to.re "C")) (str.to.re "")))))
(assert (str.in.re y (re.* (re.union (re.* (str.to.re "r")) (str.to.re "x")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 11))
(check-sat)
