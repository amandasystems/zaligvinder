(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.union (re.+ (str.to.re "K")) (str.to.re "")))))
(assert (str.in.re y (re.+ (re.union (re.* (str.to.re "2")) (str.to.re "a")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 4))
(check-sat)
