(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.+ (re.union (str.to.re "a") (re.+ (str.to.re "b"))))))
(assert (str.in.re y (re.+ (re.union (str.to.re "") (re.* (str.to.re "b"))))))
(assert (= (str.to.int x) (str.to.int y)))
(assert (not (= x y)))
(assert (= (str.to.int x) 1))
(check-sat)
