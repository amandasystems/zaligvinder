(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.union (re.* (str.to.re "}")) (str.to.re "p")))))
(assert (str.in.re y (re.* (re.union (re.+ (str.to.re "Z")) (str.to.re "p")))))
(assert (= (str.len x) (str.to.int y)))
(assert (not (= x y)))
(assert (= (str.to.int x) 8))
(check-sat)
