(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= "" (str.++ m n)))
(assert (str.in.re n (str.to.re (str.++ x y))))
(assert (> (str.len x) (str.to.int m)))
(assert (= (str.to.int y) (str.to.int n)))
(assert (= 0 4))
(check-sat)
