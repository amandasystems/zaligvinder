(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (= (str.++ y x) (str.++ n m)))
(assert (str.in.re n (re.* (str.to.re "\\\\\\\\lX"))))
(assert (> (str.len x) (str.len m)))
(assert (= 3 (str.len n)))
(assert (= 1 (str.len y)))
(check-sat)
