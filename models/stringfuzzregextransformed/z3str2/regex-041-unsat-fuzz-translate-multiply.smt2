(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in.re x (re.union (str.to.re "^^") (re.+ (str.to.re "..")))))
(assert (str.in.re x (re.union (str.to.re "AA") (re.* (str.to.re "''\\\\rr''")))))
