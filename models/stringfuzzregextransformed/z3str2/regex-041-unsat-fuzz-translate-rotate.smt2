(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in.re x (re.union (str.to.re ".") (re.+ (str.to.re "^")))))
(assert (str.in.re x (re.union (str.to.re "'\\r'") (re.* (str.to.re "A")))))
(assert (str.in.re x (re.++ (re.* (re.+ (str.to.re "-"))) (re.++ (str.to.re "O") (str.to.re "t")))))
(check-sat)
