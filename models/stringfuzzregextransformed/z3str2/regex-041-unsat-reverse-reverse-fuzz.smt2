(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in.re x (re.union (str.to.re "a") (re.+ (str.to.re "w")))))
(assert (str.in.re x (re.union (str.to.re "c") (re.+ (str.to.re "~")))))
(assert (str.in.re x (re.++ (str.to.re "0") (re.union (re.+ (str.to.re "^")) (re.* (str.to.re ""))))))
(check-sat)
