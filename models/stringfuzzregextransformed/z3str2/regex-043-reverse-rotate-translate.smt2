(set-logic QF_S)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in.re x (re.* (re.union (str.to.re "'\x0c'") (str.to.re "`")))))
(assert (= 3 (str.len x)))
(assert (not (= x "``'\x0c'")))
(assert (not (= x "'\x0c'`'\x0c'")))
(assert (not (= x "'\x0c'``")))
(assert (not (= x "```")))
(assert (not (= x "'\x0c''\x0c'`")))
(assert (not (= x "`'\x0c'`")))
(assert (not (= x "'\x0c''\x0c''\x0c'")))
(check-sat)
